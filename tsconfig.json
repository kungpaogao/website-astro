{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "types": ["@astrojs/image/client"],
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "importsNotUsedAsValues": "error",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@assets/*": ["src/assets/*"]
    }
  }
}
